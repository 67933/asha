import java.util.*;
{
class A
{
public static void freq(int arr[],int n)
{
Map<Integer,Integer> m=new HashMap<>();
for(int i=0;i<n;i++)
{
if(m.containskey(arr[i]))
{
m.put(arr[i],m.get(arr[i])+1);
}
else
{
m.put(arr[i],1);
}
}
System.out.println(m);
}
public static void main (String args[])
{
int arr[]={1,1,2,2,3,3}
int n=arr.length;
freq(arr,n);
}
}
----------
1-2
2-2
3-2
