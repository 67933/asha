class Solution {
    public int trailingZeroes(int n) {
        int mul=1,count=0,digit=0;
    if(n==0)
        return 0;
       else
for(int i=1;i<=n;i++)
 { 
   class mul=mul*i;
  }
        while(mul>1)
     {
      digit=mul%10;
        if(digit==0)
            {
           count=count+1;
            }
        mul=mul/10;
      }
        return count;
    }
}
