import java.lang.*;
import java.util.*;
public class Sort
{
   public static void main(String args[]) 
{
      Scanner sc = new Scanner(System.in);
      System.out.println("Enter a string");
      String s1 = sc.nextLine();
      char ch[] = s1.toCharArray();
      Arrays.sort(ch);
      System.out.println(new String(ch));
   }
}
