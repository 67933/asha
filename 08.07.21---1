import java.util.*;
Class array
{
public static void main(string args[])
{
Scanner sc=new Scanner(System.in);
int r=sc.nextInt();
int c=sc.nextInt();
int arr[][]=new int[r][c];
for(int i=0;i<r;i++)
for(int j=0;j<c;j++)
arr[i][j]=sc.nextInt();

for(int j=0;j<r;j++)
{
int sum=0;
for(int i=0;i<c;i++)
{
sum+=arr[i][j];
}

System.out.println("column"+(j+1)+"sum:" +sum);
}
}
}
