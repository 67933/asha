import java.util.*;
public class main
{
public static void main(String args[])
{
int n;
Scanner sc=new Scanner(System.in)
n=sc.nextInt();
int arr[]=new int[n];
for(int i=0;i<n;i++)
{
arr[i]=sc.nextInt();
}
int prefix[]=new int[n];
prefix[0]=arr[0];
for(int i=1;i<n;i++)
{
prefix[i]=prefix[i-1]+arr[i]
}
while(3>0)
int l,r;
l=sc.nextInt();
r=sc.nextInt();
if(l==0)
System.out.print(prefix[r]);
else
System.out.print(prefix[r]-prefix[l-1]);
}
}
