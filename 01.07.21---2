class tri
{
public static void main (string args [])
{
int i,j,n=5;
for(i=1;i<=n;i++)
{
for(j=1;j<=i;j++)
{
System.out.println("*");
}
System.out.println();
}
for(i=n-1;i<=0;i--)
{
for(j=1;j<=i;j++)
{
System.out.println("*");
}
System.out.println();
}
}
}
