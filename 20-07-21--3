import java.util.*;
class A
{
public static void main(String args[])
{
Stack<Integer> st=new Stack<>();
st.push(11);
st.push(22);
st.push(33);
st.push(44);
int n=st.size();
int oddcnt=0,evencnt=0;
if(n%2==0)
{
for(int i=0;i<n;i++)
{
if(st.get(i)%2==1)
oddcnt=oddcnt+1;
else
evencnt=evencnt+1;
}
}
if(evencnt==oddcnt)
{
System.out.print("stack is balanced");
}
else
{
System.out.print("stack is unbalanced");
}
}
}
------------------------
stack is balanced
